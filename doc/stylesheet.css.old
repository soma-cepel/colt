/* BODY       { background: white; color: black} */
TD.PRE     { background: #FFFF88; color: black; white-space: pre}
TD.LISTING { background: #88FFFF; color: black; white-space: pre}
TD.LPROUT  { background: #FFBBFF; color: black; white-space: pre}
CODE       { color: blue }
TT         { color: blue } 
